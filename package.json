{
  "name": "@erhahahaa/midtrans-client-typescript",
  "version": "1.0.2",
  "description": "Unofficical typescript version of midtrans client.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erhahahaa/midtrans-client-typescript.git"
  },
  "keywords": [
    "midtrans",
    "typescript",
    "payment"
  ],
  "author": "Rahmat Hidayatullah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erhahahaa/midtrans-client-typescript/issues"
  },
  "homepage": "https://github.com/erhahahaa/midtrans-client-typescript#readme",
  "devDependencies": {
    "@types/node": "^24.9.2",
    "tsdown": "^0.15.12"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}